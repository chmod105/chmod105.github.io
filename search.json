---
layout: none
---

[
  {% for post in site.posts %}
    {
      "title": "{{ post.title }}",
      "url": "{{ post.url | absolute_url }}",
      "tags": "{{ post.tags | join: ', ' }}",
      "content": "{{ post.content | strip_html | strip_newlines }}",
      "excerpt": "{{ post.excerpt | strip_html | strip_newlines }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
